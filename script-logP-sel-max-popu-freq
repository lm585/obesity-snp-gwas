rs3810291       -147.1256       1
rs7531118       -139.5749       1
rs10783050      -194.8097       1
rs1011731       -151.536        3
rs6107853       -138.9885       1
rs1345301       -146.4219       3
rs10195252      -269.0255       1
rs13389219      -269.6038       1
rs13034723      -276.3822       1
rs2607292       -119.3916       4
rs2307111       -186.7872       1
